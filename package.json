{
  "name": "melt-api",
  "scripts": {
    "offline": "sls offline --stage local",
    "deploy:dev": "sls deploy --stage dev",
    "deploy:prod": "sls deploy --stage prod",
    "docs:bundle": "redoc-cli bundle openapi.yaml -t docs/custom.hbs -o docs/index.html --options.theme.colors.primary.main=#00b1b3",
    "docs:serve": "redoc-cli serve openapi.yaml --watch"
  },
  "devDependencies": {
    "redoc-cli": "0.13.20",
    "serverless": "3.23.0",
    "serverless-domain-manager": "6.2.0",
    "serverless-offline": "11.2.3",
    "serverless-s3-local": "0.6.22"
  },
  "author": "qazsato",
  "license": "MIT"
}
